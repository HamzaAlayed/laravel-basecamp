{
    "name": "developerh/basecamp",
    "description": "Basecamp API integration for the Laravel Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamza Alayed",
            "email": "developerh108@gmail.com"
        }
    ],
    "require": {
        "league/oauth2-client": "^2.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DeveloperH\\Basecamp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeveloperH\\Basecamp\\Providers\\BasecampServiceProvider",
                "DeveloperH\\Basecamp\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Basecamp": "DeveloperH\\Basecamp\\Facades\\Basecamp"
            }
        }
    },
    "minimum-stability": "dev"
}
